<section id='body_content'>
  <section>
    <p>
      <%= image_fu @person.photo, nil %>
    </p>
  </section>
  <ul id="people">
    <% @people.each do |person| %>
      <li>
        <%= link_to person.name, person_path(person) %>
      </li>
    <% end %>
  </ul>
</section>
<section id='side_body_content'>
  <% if @page.siblings %>
    <%= render(:partial => "/shared/menu", :locals => {
                 :dom_id => 'menu',
                 :css => 'menu',
                 :roots => @page.parent.children
               }) %>
  <% end %>
  <section>
    <h1>
      <%=raw @person.name %>
    </h1>
    <p>
      <%=raw @person.bio %>
    </p>
  </section></section>


<% content_for :body_content_left do %>

<% end %>